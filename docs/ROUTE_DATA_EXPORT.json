      "category": "tools",
      "accessLevel": 2,
      "description": "Market news and updates"
    },
    {
      "path": "/whale-tracking",
      "name": "Whale Tracking",
      "component": "WhaleTrackingPage",
      "category": "tools",
      "accessLevel": 2,
      "description": "Track large market movements"
    },
    {
      "path": "/education",
      "name": "Education",
      "component": "Education",
      "category": "tools",
      "accessLevel": 2,
      "description": "Educational resources"
    },
    {
      "path": "/tax-calculator",
      "name": "Tax Calculator",
      "component": "TaxCalculatorPage",
      "category": "tools",
      "accessLevel": 2,
      "description": "Calculate tax obligations"
    },
    {
      "path": "/scheduled-trades",
      "name": "Scheduled Trades",
      "component": "ScheduledTradesPage",
      "category": "tools",
      "accessLevel": 2,
      "description": "Manage scheduled trading orders"
    },
    {
      "path": "/settings",
      "name": "Settings",
      "component": "SettingsPage",
      "category": "management",
      "accessLevel": 1,
      "description": "Application settings and preferences"
    },
    {
      "path": "/account-management",
      "name": "Account Management",
      "component": "AccountManagementPage",
      "category": "management",
      "accessLevel": 2,
      "description": "Manage account details and security"
    },
    {
      "path": "/profile",
      "name": "User Profile",
      "component": "UserProfile",
      "category": "management",
      "accessLevel": 2,
      "description": "User profile information"
    },
    {
      "path": "/strategies",
      "name": "Strategies (Legacy)",
      "component": "Strategies",
      "category": "legacy",
      "accessLevel": 2,
      "description": "Legacy strategies page"
    },
    {
      "path": "/admin-testers",
      "name": "Admin Testers",
      "component": "AdminTestersPage",
      "category": "admin",
      "accessLevel": 3,
      "description": "Admin-only tester management"
    },
    {
      "path": "/404",
      "name": "Not Found",
      "component": "NotFound",
      "category": "error",
      "accessLevel": 0,
      "description": "404 error page"
    }
  ],
  "navigationFlow": {
    "entryPoints": [
      {
        "userState": "guest",
        "routes": ["/login", "/register"],
        "defaultRedirect": "/login"
      },
      {
        "userState": "newUser",
        "routes": ["/onboarding"],
        "defaultRedirect": "/onboarding"
      },
      {
        "userState": "activeUser",
        "routes": ["/dashboard"],
        "defaultRedirect": "/dashboard"
      }
    ],
    "redirectRules": [
      {
        "from": "/",
        "to": "/dashboard",
        "condition": "authenticated && onboarded"
      },
      {
        "from": "/",
        "to": "/onboarding", 
        "condition": "authenticated && !onboarded"
      },
      {
        "from": "/",
        "to": "/login",
        "condition": "!authenticated"
      },
      {
        "from": "/onboarding",
        "to": "/dashboard",
        "condition": "onboarded"
      },
      {
        "from": "*",
        "to": "/onboarding",
        "condition": "authenticated && !onboarded && !allowedRoute"
      },
      {
        "from": "*",
        "to": "/login",
        "condition": "!authenticated && !publicRoute"
      }
    ]
  },
