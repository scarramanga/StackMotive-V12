{
  "application": {
    "name": "StackMotive",
    "version": "1.0",
    "description": "Trading and Portfolio Management Platform"
  },
  "userStates": [
    {
      "id": "guest",
      "name": "Guest User",
      "description": "Unauthenticated user",
      "accessLevel": 0,
      "allowedRoutes": ["/login", "/register"]
    },
    {
      "id": "newUser",
      "name": "New User", 
      "description": "Authenticated but incomplete onboarding",
      "accessLevel": 1,
      "allowedRoutes": ["/onboarding", "/settings", "/logout"]
    },
    {
      "id": "activeUser",
      "name": "Active User",
      "description": "Authenticated with completed onboarding",
      "accessLevel": 2,
      "allowedRoutes": ["*"]
    },
    {
      "id": "admin",
      "name": "Admin User",
      "description": "Active user with admin privileges",
      "accessLevel": 3,
      "allowedRoutes": ["*", "/admin-testers"]
    }
  ],
  "routes": [
    {
      "path": "/",
      "name": "Home",
      "component": "Dashboard",
      "redirect": "/dashboard",
      "category": "core",
      "accessLevel": 2,
      "description": "Default route redirects to dashboard"
    },
    {
      "path": "/login",
      "name": "Login",
      "component": "Login",
      "category": "auth",
      "accessLevel": 0,
      "description": "User authentication page"
    },
    {
      "path": "/register", 
      "name": "Register",
      "component": "Register",
      "category": "auth",
      "accessLevel": 0,
      "description": "New user registration page"
    },
    {
      "path": "/onboarding",
      "name": "Onboarding",
      "component": "OnboardingPage",
      "category": "onboarding",
      "accessLevel": 1,
      "description": "Multi-step user onboarding flow",
      "steps": [
        {"id": 1, "name": "Welcome", "component": "StepWelcome"},
        {"id": 2, "name": "Portfolio", "component": "StepPortfolio"},
        {"id": 3, "name": "Personal Info", "component": "StepPersonalInfo"},
        {"id": 4, "name": "Tax Info", "component": "StepTaxInfo"},
        {"id": 5, "name": "Summary", "component": "StepSummary"}
      ]
    },
    {
      "path": "/dashboard",
      "name": "Dashboard",
      "component": "Dashboard", 
      "category": "core",
      "accessLevel": 2,
      "description": "Main application dashboard"
    },
    {
      "path": "/trading",
      "name": "Trading Overview",
      "component": "Trading",
      "category": "trading",
      "accessLevel": 2,
      "description": "Trading section overview"
    },
    {
      "path": "/trading/trade",
      "name": "Trade View",
      "component": "TradeView",
      "category": "trading",
      "accessLevel": 2,
      "description": "Execute trades interface"
    },
    {
      "path": "/trading/strategies",
      "name": "Trading Strategies",
      "component": "StrategiesPage",
      "category": "trading",
      "accessLevel": 2,
      "description": "Manage trading strategies"
    },
    {
      "path": "/trading/ai-strategy-builder",
      "name": "AI Strategy Builder",
      "component": "AIStrategyBuilder",
      "category": "trading",
      "accessLevel": 2,
      "description": "Build AI-powered trading strategies"
    },
    {
      "path": "/paper-trading/new",
      "name": "New Paper Trading Account",
      "component": "NewPaperTradingAccount",
      "category": "paperTrading",
      "accessLevel": 2,
      "description": "Create new paper trading account"
    },
    {
      "path": "/paper-trading/dashboard",
      "name": "Paper Trading Dashboard",
      "component": "PaperTradingDashboard",
      "category": "paperTrading",
      "accessLevel": 2,
      "description": "Paper trading management dashboard"
    },
    {
      "path": "/paper-trading/strategy",
      "name": "Strategy Selector",
      "component": "StrategySelector",
      "category": "paperTrading",
      "accessLevel": 2,
      "description": "Select strategies for paper trading"
    },
    {
      "path": "/analytics",
      "name": "Analytics Overview",
      "component": "Analytics",
      "category": "analytics",
      "accessLevel": 2,
      "description": "Portfolio analytics overview"
    },
    {
      "path": "/advanced-analytics",
      "name": "Advanced Analytics",
      "component": "AdvancedAnalyticsPage",
      "category": "analytics",
      "accessLevel": 2,
      "description": "Advanced analytics and insights"
    },
    {
      "path": "/analysis/technical",
      "name": "Technical Analysis",
      "component": "TechnicalAnalysisPage",
      "category": "analytics",
      "accessLevel": 2,
      "description": "Technical market analysis tools"
    },
    {
      "path": "/analysis/sentiment",
      "name": "Market Sentiment",
      "component": "MarketSentimentPage",
      "category": "analytics",
      "accessLevel": 2,
      "description": "Market sentiment analysis"
    },
    {
      "path": "/analysis/portfolio",
      "name": "Portfolio Analysis",
      "component": "Analytics",
      "category": "analytics",
      "accessLevel": 2,
      "description": "Portfolio-specific analysis"
    },
    {
      "path": "/combined-portfolio",
